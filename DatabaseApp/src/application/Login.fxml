<AnchorPane fx:id="paneLogin" prefHeight="150.0" prefWidth="275.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="50.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
	<children>
		<Label layoutY="5.0" text="URL" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0" />
		<Label layoutY="35.0" text="Username" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="35.0" />
		<Label layoutY="65.0" text="Password" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="65.0" />
